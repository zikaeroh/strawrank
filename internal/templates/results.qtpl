{% code
type ResultsPage struct {
    BasePage
    Question string
    Content string
}
%}

{% func (p *ResultsPage) PageTitle() %}
	StrawRank - Results - {%s p.Question %}
{% endfunc %}

{% func (p *ResultsPage) PageBody() %}
    <div class="row">
        <div class="col">
            <pre>
{%s p.Content %}
            </pre>
        </div>
    </div>
{% endfunc %}
